#!/bin/sh
# postrm script for bunsen-meta-all

set -e

BL_CONFDIR=/etc/bunsen
BL_INST="${BL_CONFDIR}/bunsen_install"

REMOVE_STRING="Removed bunsen-meta-all at $(date --utc +%FT%T)"

case $1 in
remove|purge)
    echo "$REMOVE_STRING" >> "$BL_INST"
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0

